    "description": "Final Project for TAP Cohort SD-0224",
    "main": "./dist/server.js",
    "scripts": {
        "migrate": "npx sequelize-cli db:migrate",
        "seed": "npx sequelize-cli db:seed:all",
        "migrate:undo": "npx sequelize-cli db:migrate:undo",
        "migrate:undo-all": "npx sequelize-cli db:migrate:undo:all",
        "clean": "rimraf dist",
        "lint": "tslint -c tslint.json -p tsconfig.json --fix",
        "tsc": "tsc",
        "build": "npm-run-all clean lint tsc",
        "prestart": "npm run build",
        "start": "node ./dist/server.js",
        "preserve": "npm run build",
        "serve": "nodemon -w ts-node ./server.ts",
        "test": "npx jest--forceExit  --detectOpenHandles __tests__/allTests.test.ts",
        "test_pr": "npx jest --forceExit  --detectOpenHandles __tests__/baselineCheck.test.ts"
    },
